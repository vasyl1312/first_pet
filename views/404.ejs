<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- favicon -->
    <link rel="shortcut icon" type="image/png" href="../assets/calzone.png" />
    <script
      src="https://code.jquery.com/jquery-3.6.3.min.js"
      integrity="sha256-pvPw+upLPUjgMXY0G+8O0xUf+/Im1MZjXxxgOcBQBXU="
      crossorigin="anonymous"
    ></script>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <script src="https://kit.fontawesome.com/a076d05399.js" crossorigin="anonymous"></script>
    <script>
      $(function () {
        $('#header').load('./headers/mainHeader.ejs')
        $('#productsDropdown').load('./headers/product.ejs')
        $('#authRoutes').load('./headers/authRoute.ejs')
        $('#loader').load('./headers/loader.ejs')
        $('#logo').load('./headers/logo.ejs')
        $('#homeRoute').load('./headers/homeRoute.ejs')
        $('#loginRoute').load('./headers/loginRoute.ejs')
        $('#hero').load('./headers/hero.ejs')
        $('#copyright').load('./headers/copyright.ejs')
        $('#scripts').load('./headers/scripts.ejs')
      })
    </script>
    <div id="header"></div>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    />
  </head>
  <body>
    <!--PreLoader-->
    <a id="loader"></a>
    <!--PreLoader Ends-->

    <!-- header -->
    <div class="top-header-area" id="sticker">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 col-sm-12 text-center">
            <div class="main-menu-wrap">
              <!-- logo -->
              <a id="logo"></a>
              <!-- logo -->

              <!-- menu start -->
              <nav class="main-menu">
                <ul>
                  <!-- <li class="current-list-item"> -->
                  <li id="homeRoute"></li>
                  <!-- </li> -->
                  <li id="productsDropdown"></li>
                  <% if (isAuth) {%>
                  <a id="authRoutes"></a>
                  <%} else {%>
                  <li id="loginRoute"></li>
                  <%}%>
                </ul>
              </nav>
              <div class="mobile-menu"></div>
              <!-- menu end -->
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- end header -->

    <!-- hero area -->
    <a id="hero"></a>
    <!-- end hero area -->

    <!-- error section -->
    <div class="full-height-section error-section">
      <div class="full-height-tablecell">
        <div class="container">
          <div class="row">
            <div class="col-lg-8 offset-lg-2 text-center">
              <div class="section-title">
                <h3><span class="orange-text">Oops! </span>Not Found.</h3>
                <div class="error-text">
                  <i class="far fa-dizzy"></i>
                  <p>The page you requested for <b>is not found.</b></p>
                  <br />
                </div>
                <a href="/" class="boxed-btn">Back to Home</a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- end error section -->

    <!-- copyright -->
    <a id="copyright"></a>
    <!-- end copyright -->
    <a id="scripts"></a>
  </body>
</html>
